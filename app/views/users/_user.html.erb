<li>
<div class="icon">
  <% if user.icon.attached? %>
    <%= image_tag user.icon_image %>
  <% else %>
    <%= image_tag("default_user.jpg", size: 50)%>
  <% end %>
  <%= link_to user.name, user %>
  <% if current_user.admin? && !current_user?(user)%>
    <%= link_to "削減", user, method: :delete,
                                data: { confirm: "削減しますか？" } %>
  <% end %>
</div>
</li>
  